#include <stdio.h>
#include <stdlib.h>
#include "debugmalloc.h"

int main()
{
    printf("Kerem a szamokat!\n");
    int n = 0;
    double *t = NULL;
    double x;
    while(scanf("%lf",&x) == 1 && x !=-1)
    {
        double *tseged = (double*) malloc(sizeof(double)*(n + 1));
        for(int i=0;i<n;++i)
        {
            tseged[i] = t[i];
        }
        free(t);
        t = tseged;
        t[n] = x;
        ++n;
    }

    for(int i=n-1;i>=0;--i)
    {
        printf("%g\n", t[i]);
    }
    free(t);
    return 0;
}
