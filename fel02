#include <stdio.h>
#include <stdlib.h>
#include <math.h>

double kob(double x)
{
    return x*x*x;
}
double abszolut(double x)
{
    if(x<0)
    {
        x = x*-1;
    }
    return x;
}
int main()
{
    double a = -1;
    while(a < 1)
    {
        printf("%.4lf %.4lf %.4lf %.4lf\n",a,kob(a),abszolut(a),sin(a));
        a = a + 0.1;
    }
    return 0;
}
